---
    extends: "airbnb"
    parser: "babel-eslint"
    globals:
        window: false
        require: false
        document: false
        t: false
        setInterval: false
        clearInterval: false
        localStorage: false
        location: false
        process: false
        __dirname: false
        module: false
        console: false
    ecmaFeatures:
        jsx: true
    plugins:
        - react
    rules:
        max-len: [1, 80, 4]
        indent: [2, 4, {"SwitchCase": 1}]

        "react/display-name": 1
        "react/jsx-no-duplicate-props": 1
        "react/no-danger": 1
        "react/require-extension": 1
